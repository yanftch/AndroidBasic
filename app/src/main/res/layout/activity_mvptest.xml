<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.yanftch.basic.mvp.MVPTestActivity">

    <Button
        android:id="@+id/btnGetHome"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="gethome"/>

    <TextView
        android:id="@+id/tvHomeData"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        android:textColor="@color/color_2480d4"/>

    <TextView
        android:id="@+id/tvHomeData2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        android:textColor="@color/color_999999"/>

    <com.yanftch.basic.view.ColumnView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</LinearLayout>
